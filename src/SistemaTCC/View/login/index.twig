<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <base href="{{ app.url('/') }}">
    <title>SistemaTCC</title>
    <link rel="stylesheet" href="./lib/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="./lib/bootstrap/dist/css/bootstrap.min.css">
    <!-- css do filipe --> 
    <link rel="stylesheet" href="./app/login/login.css">

    <!-- styles -->
    {% block styles %}
    {% endblock %}
    <!-- /styles -->
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
  </head>
  <body>
<!-- Login desenvolvido por Filipe de leonel batista!-->

<div class="container centro">
    <div class="row">
        <div class="col-sm-6 col-md-4 col-md-offset-4">
            <h1 class="text-center login-title"><a href="./creditos"><img src="./app/login/img/logo.png" height="50"></a> Sistema TCC</h1>
            <div class="account-wall">
                <form class="form-signin">
                <input type="text" class="form-control" placeholder="CGU" required autofocus>
                <input type="password" class="form-control" placeholder="Senha" required>
                <button class="btn btn-lg btn-primary btn-block" type="submit">
                    Login</button>
                </form>
            </div>
        </div>
    </div>
</div>
    <script src="./lib/jquery/dist/jquery.min.js"></script>
    <script src="./lib/bootstrap/dist/js/bootstrap.min.js"></script>
  </body>
</html>
